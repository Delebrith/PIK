<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="liquidbase" id="002">
        <createTable tableName="project">
            <column name="id" type="BIGINT(19)">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="name" type="nvarchar(200)">
                <constraints nullable="false" />
            </column>
            <column name="description" type="nvarchar(1000)">
                <constraints nullable="false" />
            </column>
            <column name="is_paid" type="boolean">
                <constraints nullable="false" />
            </column>
            <column name="is_graduate_work" type="boolean">
                <constraints nullable="false" />
            </column>
            <column name="number_of_participants" type="int">
                <constraints nullable="false" />
            </column>
            <column name="status" type="varchar(100)">
                <constraints nullable="false" />
            </column>

        </createTable>
        <addAutoIncrement columnDataType="BIGINT"
                          columnName="id"
                          incrementBy="1"
                          startWith="100"
                          tableName="project"/>
        <loadData encoding="UTF-8"
                  file="db/data/project.csv"
                  quotchar="'"
                  schemaName="public"
                  separator=","
                  tableName="project">
        </loadData>
    </changeSet>
</databaseChangeLog>